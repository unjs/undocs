{
  "name": "undocs",
  "version": "0.2.6",
  "repository": "unjs/undocs",
  "license": "MIT",
  "type": "module",
  "bin": {
    "undocs": "./cli/main.mjs"
  },
  "files": [
    "app",
    "cli",
    "schema",
    "!app/**/node_modules",
    "!app/**/tsconfig.json"
  ],
  "scripts": {
    "build": "true",
    "dev": "bun run docs:dev",
    "docs:build": "bun run undocs build docs",
    "docs:dev": "NUXT_DOCS_DEV=1 bun run undocs dev docs",
    "lint": "eslint --cache --ext .ts,.js,.mjs,.cjs . && prettier -c app cli schema",
    "lint:fix": "eslint --cache --ext .ts,.js,.mjs,.cjs . --fix && prettier -c -w app cli schema",
    "prepack": "bun run build",
    "profile:0x": "bunx 0x -o cli/main.mjs dev docs",
    "profile:0x-kernel": "bun run profile:0x --kernel-tracing",
    "profile:cpu": "rm -rf .profile && node --cpu-prof --diagnostic-dir ../.profile bin/cli.mjs dev docs",
    "release": "bun run lint && changelogen --release && npm publish && git push --follow-tags",
    "template:dev": "bun run undocs dev template",
    "undocs": "./cli/main.mjs"
  },
  "dependencies": {
    "@nuxt/content": "^2.12.0",
    "@nuxt/ui": "^2.13.0",
    "@nuxt/ui-pro": "^0.7.5",
    "@nuxtjs/fontaine": "^0.4.1",
    "@nuxtjs/google-fonts": "^3.1.3",
    "@nuxtjs/plausible": "^0.2.4",
    "@nuxtjs/seo": "^2.0.0-rc.8",
    "@nuxtjs/tailwindcss": "^6.11.3",
    "automd": "^0.3.4",
    "c12": "^1.7.0",
    "citty": "^0.1.5",
    "consola": "^3.2.3",
    "defu": "^6.1.4",
    "is-buffer": "^2.0.5",
    "nuxi": "^3.10.1",
    "nuxt": "^3.10.2",
    "nuxt-build-cache": "^0.1.1",
    "pkg-types": "^1.0.3",
    "scule": "^1.3.0",
    "tailwindcss": "^3.4.1",
    "theme-colors": "^0.1.0",
    "unctx": "^2.3.1",
    "unstorage": "^1.10.1",
    "vue": "~3.3.13",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "^0.2.0",
    "@types/node": "^20.11.16",
    "changelogen": "^0.5.5",
    "eslint": "^8.56.0",
    "eslint-config-unjs": "^0.2.1",
    "jiti": "^1.21.0",
    "prettier": "^3.2.5",
    "typescript": "^5.3.3",
    "vue-tsc": "^1.8.27"
  },
  "packageManager": "bun@1.0.26"
}
