{
  "name": "undocs",
  "version": "0.4.4",
  "repository": "unjs/undocs",
  "license": "MIT",
  "type": "module",
  "bin": {
    "undocs": "./cli/main.mjs"
  },
  "files": [
    "app",
    "cli",
    "schema",
    "!app/**/node_modules",
    "!app/**/tsconfig.json"
  ],
  "scripts": {
    "build": "true",
    "dev": "pnpm run docs:dev",
    "docs:build": "pnpm run undocs build docs",
    "docs:dev": "NUXT_DOCS_DEV=1 pnpm run undocs dev docs",
    "lint": "eslint --cache . && prettier -c app cli schema",
    "lint:fix": "eslint --cache . --fix && prettier -c -w app cli schema",
    "prepack": "pnpm run build",
    "profile:0x": "pnpx 0x -o cli/main.mjs dev docs",
    "profile:0x-kernel": "pnpm run profile:0x --kernel-tracing",
    "profile:cpu": "rm -rf .profile && node --cpu-prof --diagnostic-dir ../.profile bin/cli.mjs dev docs",
    "release": "pnpm run lint && changelogen --release && npm publish && git push --follow-tags",
    "template:dev": "pnpm run undocs dev template",
    "undocs": "./cli/main.mjs"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.23",
    "@iconify-json/logos": "^1.2.9",
    "@iconify-json/simple-icons": "^1.2.52",
    "@nuxt/content": "^3.7.1",
    "@nuxt/fonts": "^0.11.4",
    "@nuxt/ui": "4.0.0-alpha.2",
    "@nuxtjs/plausible": "^2.0.1",
    "@resvg/resvg-wasm": "^2.6.2",
    "automd": "^0.4.2",
    "c12": "^3.3.0",
    "citty": "^0.1.6",
    "consola": "^3.4.2",
    "defu": "^6.1.4",
    "is-buffer": "^2.0.5",
    "md4w": "^0.2.7",
    "mermaid": "^11.12.0",
    "nitropack": "^2.12.6",
    "nuxi": "^3.28.0",
    "nuxt": "^4.1.2",
    "nuxt-build-cache": "^0.1.1",
    "nuxt-llms": "^0.1.3",
    "pkg-types": "^2.3.0",
    "scule": "^1.3.0",
    "shiki": "^3.12.2",
    "tailwindcss": "^4.1.13",
    "unctx": "^2.4.1",
    "unstorage": "^1.17.1",
    "vue": "^3.5.21",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "^1.9.0",
    "@nuxt/image": "^1.11.0",
    "@types/node": "^24.5.2",
    "changelogen": "^0.6.2",
    "eslint": "^9.35.0",
    "eslint-config-unjs": "^0.5.0",
    "jiti": "^2.5.1",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2",
    "vue-tsc": "^3.0.7"
  },
  "packageManager": "pnpm@10.17.0"
}
