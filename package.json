{
  "name": "undocs",
  "version": "0.4.16",
  "license": "MIT",
  "repository": "unjs/undocs",
  "bin": {
    "undocs": "./cli/main.mjs"
  },
  "files": [
    "app",
    "cli",
    "schema",
    "!app/**/node_modules",
    "!app/**/tsconfig.json"
  ],
  "type": "module",
  "scripts": {
    "build": "true",
    "dev": "pnpm run docs:dev",
    "docs:build": "pnpm run undocs build docs",
    "docs:dev": "NUXT_DOCS_DEV=1 pnpm run undocs dev docs",
    "lint": "oxlint && oxfmt --check",
    "fmt": "oxlint --fix && oxfmt",
    "prepack": "pnpm run build",
    "profile:0x": "pnpx 0x -o cli/main.mjs dev docs",
    "profile:0x-kernel": "pnpm run profile:0x --kernel-tracing",
    "profile:cpu": "rm -rf .profile && node --cpu-prof --diagnostic-dir ../.profile bin/cli.mjs dev docs",
    "release": "pnpm run lint && changelogen --release && npm publish && git push --follow-tags",
    "template:dev": "pnpm run undocs dev template",
    "undocs": "./cli/main.mjs"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.23",
    "@nuxt/content": "^3.11.0",
    "@nuxt/fonts": "^0.13.0",
    "@nuxt/ui": "^4.4.0",
    "@nuxtjs/plausible": "^2.0.1",
    "@resvg/resvg-wasm": "^2.6.2",
    "automd": "^0.4.3",
    "c12": "^3.3.3",
    "citty": "^0.2.0",
    "consola": "^3.4.2",
    "defu": "^6.1.4",
    "md4w": "^0.2.7",
    "mdream": "^0.15.3",
    "mermaid": "^11.12.2",
    "motion-v": "^1.10.2",
    "nuxi": "^3.32.0",
    "nuxt": "^4.3.0",
    "nuxt-llms": "^0.2.0",
    "pathe": "^2.0.3",
    "scule": "^1.3.0",
    "shiki": "^3.21.0",
    "tailwindcss": "^4.1.18",
    "unctx": "^2.5.0",
    "vue": "^3.5.27"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "^1.13.0",
    "@types/node": "^25.1.0",
    "changelogen": "^0.6.2",
    "nitropack": "^2.13.1",
    "oxfmt": "^0.27.0",
    "oxlint": "^1.42.0",
    "typescript": "^5.9.3",
    "vue-tsc": "^3.2.4"
  },
  "resolutions": {
    "undocs": "link:."
  },
  "packageManager": "pnpm@10.28.2"
}
